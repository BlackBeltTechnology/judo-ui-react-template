const isFormUpdateable = useCallback(() => {
    return {{ boolValue page.dataElement.isUpdatable }} && typeof data?.__updateable === 'boolean' && data?.__updateable;
}, [data]);

const isFormDeleteable = useCallback(() => {
    return {{ boolValue page.dataElement.isDeletable }} && typeof data?.__deleteable === 'boolean' && data?.__deleteable;
}, [data]);
