import type { AlternativeApplicationGenerator } from './interfaces';

export const generateAlternativeApplications: AlternativeApplicationGenerator = (t: Function) => {
    return {
        {{# each (getAlternativeApplications application applications) as |app| }}
            '{{ plainName app.fQName }}': t('applications.{{ app.actor.simpleName }}', { defaultValue: '{{ app.actor.simpleName }}' }) as string,
        {{/ each }}
    };
};
