{
    id: '{{ createId item }}',
    {{# unless (isNavItemAGroup item) }}
        to: '{{ getPageRoute item.target }}',
    {{/ unless }}
    {{# if item.icon }}
        icon: '{{ item.icon.name }}',
    {{/ if }}
    {{# if item.label }}
        label: '{{ item.label }}',
    {{/ if }}
    {{# if item.hiddenBy }}
        hiddenBy: '{{ item.hiddenBy }}',
    {{/ if }}
    items: [
        {{# each item.items as |subItem| }}
            {{> actor/src/menu-items-recurse.fragment.hbs item=subItem  }}
        {{/ each }}
    ],
},
