{{> fragment.header.hbs }}
{{# unless (hasElements (getUniquePageActions page)) }}
    // page has no actions
    export {};
{{/ unless }}
{{# each (getUniquePageActions page) as |action| }}
    {{# if (actionHasInputForm action) }}
        export * from './{{ pageActionFormPathSuffix action }}';
    {{/ if }}
    export * from './{{ pageActionPathSuffix action }}';
{{/ each }}
