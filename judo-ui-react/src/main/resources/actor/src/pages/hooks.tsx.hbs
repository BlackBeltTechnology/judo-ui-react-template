{{# if page.dataElement }}
    {{# if page.dataElement.isAccess }}
        {{# if page.isPageTypeCreate }}
            {{> actor/src/pages/access/create/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeTable }}
            {{> actor/src/pages/access/table/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeView }}
            {{> actor/src/pages/access/view/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeDashboard }}
            {{# if page.dataElement.isCollection }}
                {{> actor/src/pages/access/table/hooks.hbs }}
            {{ else }}
                {{> actor/src/pages/access/view/hooks.hbs }}
            {{/ if }}
        {{/ if }}
    {{ else }}
        {{# if page.isPageTypeCreate }}
            {{> actor/src/pages/relation/create/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeTable }}
            {{> actor/src/pages/relation/table/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeView }}
            {{> actor/src/pages/relation/view/hooks.hbs }}
        {{/ if }}
        {{# if page.isPageTypeOperationOutput }}
            {{> actor/src/pages/operation-output/view/hooks.hbs }}
        {{/ if }}
    {{/ if }}
{{ else }}
    {{> actor/src/pages/page-fallback.hooks.tsx.hbs }}
{{/ if }}
